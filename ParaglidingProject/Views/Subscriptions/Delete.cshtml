@model ParaglidingProject.SL.Core.Subscription.NS.transferObjects.SubscriptionDto

@{
    ViewData["Title"] = "Supprimer";
}

<h1>Supprimer l'année de cotisation @Model.Id?</h1>

<h3>Etes-vous sûr de vouloir supprimer?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Amount)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Amount)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Supprimer" class="btn btn-danger add" /> |
        <a class="styleLink" asp-action="Index">Retour</a>
    </form>
</div>
